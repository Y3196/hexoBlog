;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,QAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA,GAAA,KAAA,GAAA,KAAA,CAAA,IAAA;AACA;;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,qBAAA,CAAA,MAAA,EAAA;AAAA;;AACA,UAAA,KAAA,CAAA,OAAA;;AACA,+BAAA,KAAA,CAAA,WAAA,EAAA,IAAA,6CAAA,IAAA,CAAA,IAAA,CAAA,qBAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,QAAA;AACA;AACA,OApBA;AAqBA;AAvBA,GAlBA;AA2CA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,UAAA,KAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,SAAA,IAAA,aAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,SAAA;AACA;AACA,OAJA;AAKA,aAAA,qBAAA,KAAA,GAAA,mCAAA;AACA;AATA;AA3CA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- 标签或分类名 -->\n    <div class=\"banner\" :style=\"cover\">\n      <h1 class=\"banner-title animated fadeInDown\">{{ title }} - {{ name }}</h1>\n    </div>\n    <div class=\"article-list-wrapper\">\n      <v-row>\n        <v-col md=\"4\" cols=\"12\" v-for=\"item of articleList\" :key=\"item.id\">\n          <!-- 文章 -->\n          <v-card class=\"animated zoomIn article-item-card\">\n            <div class=\"article-item-cover\">\n              <router-link :to=\"'/articles/' + item.id\">\n                <!-- 缩略图 -->\n                <v-img\n                  class=\"on-hover\"\n                  width=\"100%\"\n                  height=\"100%\"\n                  :src=\"item.articleCover\"\n                />\n              </router-link>\n            </div>\n            <div class=\"article-item-info\">\n              <!-- 文章标题 -->\n              <div>\n                <router-link :to=\"'/articles/' + item.id\">\n                  {{ item.articleTitle }}\n                </router-link>\n              </div>\n              <div style=\"margin-top:0.375rem\">\n                <!-- 发表时间 -->\n                <v-icon size=\"20\">mdi-clock-outline</v-icon>\n                {{ item.createTime | date }}\n                <!-- 文章分类 -->\n                <router-link\n                  :to=\"'/categories/' + item.categoryId\"\n                  class=\"float-right\"\n                >\n                  <v-icon>mdi-bookmark</v-icon>{{ item.categoryName }}\n                </router-link>\n              </div>\n            </div>\n            <!-- 分割线 -->\n            <v-divider></v-divider>\n            <!-- 文章标签 -->\n            <div class=\"tag-wrapper\">\n              <router-link\n                :to=\"'/tags/' + tag.id\"\n                class=\"tag-btn\"\n                v-for=\"tag of item.tagDTOList\"\n                :key=\"tag.id\"\n              >\n                {{ tag.tagName }}\n              </router-link>\n            </div>\n          </v-card>\n        </v-col>\n      </v-row>\n      <!-- 无限加载 -->\n      <infinite-loading @infinite=\"infiniteHandler\">\n        <div slot=\"no-results\" />\n        <div slot=\"no-more\" />\n      </infinite-loading>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  created() {\n    const path = this.$route.path;\n    if (path.indexOf(\"/categories\") != -1) {\n      this.title = \"分类\";\n    } else {\n      this.title = \"标签\";\n    }\n  },\n  data: function() {\n    return {\n      current: 1,\n      size: 10,\n      articleList: [],\n      name: \"\",\n      title: \"\"\n    };\n  },\n  methods: {\n    infiniteHandler($state) {\n      this.axios\n        .get(\"/api/articles/condition\", {\n          params: {\n            categoryId: this.$route.params.categoryId,\n            tagId: this.$route.params.tagId,\n            current: this.current\n          }\n        })\n        .then(({ data }) => {\n          if (data.data.name) {\n            this.name = data.data.name;\n            document.title = this.title + \" - \" + this.name;\n          }\n          if (data.data.articlePreviewDTOList.length) {\n            this.current++;\n            this.articleList.push(...data.data.articlePreviewDTOList);\n            $state.loaded();\n          } else {\n            $state.complete();\n          }\n        });\n    }\n  },\n  computed: {\n    cover() {\n      var cover = \"\";\n      this.$store.state.blogInfo.pageList.forEach(item => {\n        if (item.pageLabel == \"articleList\") {\n          cover = item.pageCover;\n        }\n      });\n      return \"background: url(\" + cover + \") center center / cover no-repeat\";\n    }\n  }\n};\n</script>\n\n<style scoped>\n@media (min-width: 760px) {\n  .article-list-wrapper {\n    max-width: 1106px;\n    margin: 370px auto 1rem auto;\n  }\n  .article-item-card:hover {\n    transition: all 0.3s;\n    box-shadow: 0 4px 12px 12px rgba(7, 17, 27, 0.15);\n  }\n  .article-item-card:not(:hover) {\n    transition: all 0.3s;\n  }\n  .article-item-card:hover .on-hover {\n    transition: all 0.6s;\n    transform: scale(1.1);\n  }\n  .article-item-card:not(:hover) .on-hover {\n    transition: all 0.6s;\n  }\n  .article-item-info {\n    line-height: 1.7;\n    padding: 15px 15px 12px 18px;\n    font-size: 15px;\n  }\n}\n@media (max-width: 759px) {\n  .article-list-wrapper {\n    margin-top: 230px;\n    padding: 0 12px;\n  }\n  .article-item-info {\n    line-height: 1.7;\n    padding: 15px 15px 12px 18px;\n  }\n}\n.article-item-card {\n  border-radius: 8px !important;\n  box-shadow: 0 4px 8px 6px rgba(7, 17, 27, 0.06);\n}\n.article-item-card a {\n  transition: all 0.3s;\n}\n.article-item-cover {\n  height: 220px;\n  overflow: hidden;\n}\n.article-item-card a:hover {\n  color: #8e8cd8;\n}\n.tag-wrapper {\n  padding: 10px 15px 10px 18px;\n}\n.tag-wrapper a {\n  color: #fff !important;\n}\n.tag-btn {\n  display: inline-block;\n  font-size: 0.725rem;\n  line-height: 22px;\n  height: 22px;\n  border-radius: 10px;\n  padding: 0 12px !important;\n  background: linear-gradient(to right, #bf4643 0%, #6c9d8f 100%);\n  opacity: 0.6;\n  margin-right: 0.5rem;\n}\n</style>\n"],"sourceRoot":"src/views/article"}]}