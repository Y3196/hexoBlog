A;;;AACA,cAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,YAAA;;AACA,cAAA,MAAA,CAAA,MAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,SAAA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAAA;AACA,aALA,MAKA;AACA,cAAA,MAAA,CAAA,MAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,OAAA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAAA;AACA;AACA,WAVA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,KAAA,CAAA,cAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,MAAA,CAAA;AAAA,cAAA,IAAA,EAAA,OAAA;AAAA,cAAA,OAAA,EAAA;AAAA,aAAA;AACA,WAbA;AAcA,SAnBA,MAmBA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA;AAAA,YAAA,IAAA,EAAA,OAAA;AAAA,YAAA,OAAA,EAAA,IAAA,CAAA;AAAA,WAAA;AACA;AACA,OAvBA,EAuBA,KAvBA,CAuBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,qBAAA,EAAA,KAAA,EADA,CACA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA;AAAA,UAAA,IAAA,EAAA,OAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,OA1BA;AA2BA;AA3FA,GAZA;AA0GA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA;AACA,MAAA,GADA,eACA,KADA,EACA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,OAHA;AAIA,MAAA,GAJA,iBAIA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,YAAA;AACA;AANA,KADA;AASA,IAAA,QATA,sBASA;AACA,UAAA,WAAA,GAAA,QAAA,CAAA,eAAA,CAAA,WAAA;;AACA,UAAA,WAAA,GAAA,GAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAfA,GA1GA;AA2HA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,oBACA,KADA,EACA;AACA,UAAA,GAAA,GAAA,+DAAA;;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,GAAA,IAAA;AACA;AACA;AARA;AA3HA,CAAA","sourcesContent":["<template>\n  <v-dialog v-model=\"registerFlag\" :fullscreen=\"isMobile\" max-width=\"460\">\n    <v-card class=\"login-container\" style=\"border-radius:4px\">\n      <v-icon class=\"float-right\" @click=\"registerFlag = false\">\n        mdi-close\n      </v-icon>\n      <div class=\"login-wrapper\">\n        <!-- 用户名 -->\n        <v-text-field\n          v-model=\"username\"\n          label=\"邮箱号\"\n          placeholder=\"请输入您的邮箱号\"\n          clearable\n          @keyup.enter=\"register\"\n        />\n        <!-- 验证码 -->\n        <div class=\"mt-7 send-wrapper\">\n          <v-text-field\n            maxlength=\"6\"\n            v-model=\"code\"\n            label=\"验证码\"\n            placeholder=\"请输入6位验证码\"\n            @keyup.enter=\"register\"\n          />\n          <v-btn text small :disabled=\"flag\" @click=\"sendCode\">\n            {{ codeMsg }}\n          </v-btn>\n        </div>\n        <!-- 密码 -->\n        <v-text-field\n          v-model=\"password\"\n          class=\"mt-7\"\n          label=\"密码\"\n          placeholder=\"请输入您的密码\"\n          @keyup.enter=\"register\"\n          :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n          :type=\"show ? 'text' : 'password'\"\n          @click:append=\"show = !show\"\n        />\n        <!-- 注册按钮 -->\n        <v-btn\n          class=\"mt-7\"\n          block\n          color=\"red\"\n          style=\"color:#fff\"\n          @click=\"register\"\n        >\n          注册\n        </v-btn>\n        <!-- 登录 -->\n        <div class=\"mt-10 login-tip\">\n          已有账号？<span @click=\"openLogin\">登录</span>\n        </div>\n      </div>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  data: function() {\n    return {\n      username: \"\",\n      code: \"\",\n      password: \"\",\n      flag: true,\n      codeMsg: \"发送\",\n      time: 60,\n      show: false\n    };\n  },\n  methods: {\n    openLogin() {\n      this.$store.state.registerFlag = false;\n      this.$store.state.loginFlag = true;\n    },\n    sendCode() {\n      const that = this;\n      // eslint-disable-next-line no-undef\n      var captcha = new TencentCaptcha(this.config.TENCENT_CAPTCHA, function(\n        res\n      ) {\n        if (res.ret === 0) {\n          //发送邮件\n          that.countDown();\n          that.axios\n            .get(\"/api/users/code\", {\n              params: { username: that.username }\n            })\n            .then(({ data }) => {\n              if (data.flag) {\n                that.$toast({ type: \"success\", message: \"发送成功\" });\n              } else {\n                that.$toast({ type: \"error\", message: data.message });\n              }\n            });\n        }\n      });\n      // 显示验证码\n      captcha.show();\n    },\n    countDown() {\n      this.flag = true;\n      this.timer = setInterval(() => {\n        this.time--;\n        this.codeMsg = this.time + \"s\";\n        if (this.time <= 0) {\n          clearInterval(this.timer);\n          this.codeMsg = \"发送\";\n          this.time = 60;\n          this.flag = false;\n        }\n      }, 1000);\n    },\n    register() {\n      var reg = /^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/;\n      if (!reg.test(this.username)) {\n        this.$toast({ type: \"error\", message: \"邮箱格式不正确\" });\n        return false;\n      }\n      if (this.code.trim().length != 6) {\n        this.$toast({ type: \"error\", message: \"请输入6位验证码\" });\n        return false;\n      }\n      if (this.password.trim().length < 6) {\n        this.$toast({ type: \"error\", message: \"密码不能少于6位\" });\n        return false;\n      }\n\n      const user = {\n        username: this.username,\n        password: this.password,\n        code: this.code,\n      };\n\n      this.axios.post(\"/api/register\", user).then(({ data }) => {\n        if (data.flag) {\n          // 如果注册成功，触发登录请求\n          let param = new URLSearchParams();\n          param.append(\"username\", user.username);\n          param.append(\"password\", user.password);\n          this.axios.post(\"/api/login\", param).then(({ data }) => {\n            console.log(\"Login response data:\", data); // 调试输出\n            if (data.token) {  // 确保 token 存在\n              this.$store.commit(\"login\", data.user);\n              this.$store.commit(\"setToken\", data.token);  // 保存 token\n              this.$store.commit(\"closeModel\");\n              this.$toast({ type: \"success\", message: \"登录成功\" });\n            } else {\n              this.$toast({ type: \"error\", message: \"登录失败：Token 未返回\" });\n            }\n          }).catch((error) => {\n            console.error(\"Login error:\", error);\n            this.$toast({ type: \"error\", message: \"登录请求失败，请稍后再试\" });\n          });\n        } else {\n          this.$toast({ type: \"error\", message: data.message });\n        }\n      }).catch((error) => {\n        console.error(\"Registration error:\", error);  // 捕获注册请求错误\n        this.$toast({ type: \"error\", message: \"注册请求失败，请稍后再试\" });\n      });\n    }\n\n  },\n  computed: {\n    registerFlag: {\n      set(value) {\n        this.$store.state.registerFlag = value;\n      },\n      get() {\n        return this.$store.state.registerFlag;\n      }\n    },\n    isMobile() {\n      const clientWidth = document.documentElement.clientWidth;\n      if (clientWidth > 960) {\n        return false;\n      }\n      return true;\n    }\n  },\n  watch: {\n    username(value) {\n      var reg = /^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/;\n      if (reg.test(value)) {\n        this.flag = false;\n      } else {\n        this.flag = true;\n      }\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components/model"}]}